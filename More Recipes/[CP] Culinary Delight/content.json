{
    "Format": "2.3.0",
    "ConfigSchema": { 
	//Config Options
		"flexible_recipes": { 
			"AllowValues": "On, Off",
			"Default": "On",
			"Description": "Whether recipes can accept inputs from multiple installed mods."
		}
    },
    "Changes": [

        {
            "Action": "Include",
            "FromFile": "data/objects.json, data/recipes.json, data/recipereplacements.json"
        },
        {
            "Action": "Load",
            "Target": "Mods/{{ModId}}/Dishes",
            "FromFile": "assets/foods.png"
        }
    ]
}